{
  "name": "releaf-mills-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "description": "Releaf API for PK Dumpsite solution",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "prestart": "npm run migrate:up && npm run seed:up",
    "start": "node dist/src/index.js",
    "dev": "nodemon --exec ts-node ./src/index.ts --mode development",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "cross-env WERELEAF_NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=text-summary mocha --require ts-node/register --recursive --timeout=150000 tests/index.ts --exit",
    "migrations": "source .env && db-migrate",
    "migrate:up": "db-migrate up  --config database.json",
    "migrate:down": "db-migrate down  --config database.json",
    "migrate-test:up": "db-migrate up --config database.json -e test",
    "migrate-test:down": "db-migrate down -c 100 --config database.json -e test",
    "migrate:reset": "db-migrate reset --config database.json",
    "migrate-test:reset": "db-migrate reset --config database.json -e test",
    "seed:create": "db-migrate create:seed --config database.json",
    "seed:up": "db-migrate up:seed --config database.json",
    "seed:down": "db-migrate down:seed --config database.json",
    "seed:reset": "db-migrate reset:seed --config database.json",
    "seed:reset:test": "db-migrate reset:seed --config database.json -e test",
    "seed:up:test": "db-migrate up:seed --config database.json -e test"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "mocha": "^10.8.2",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "pg-promise": "^11.10.2",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.42",
    "@types/chai": "^5.0.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/mocha": "^10.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^10.0.0",
    "chai": "^5.1.2",
    "nodemon": "^3.1.7",
    "nyc": "^17.1.0",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
